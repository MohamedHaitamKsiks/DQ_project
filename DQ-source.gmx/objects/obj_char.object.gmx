    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!global.transition &amp;&amp; !start &amp;&amp; !finish){
//gravity
scr_char_gravity();

//movment

left = -keyboard_check(global.left);
right = keyboard_check(global.right);

wall_left = place_meeting(x-abs(hspeed)-1,y,obj_solid);
wall_right = place_meeting(x+abs(hspeed)+1,y,obj_solid);

hspeed += acc*(left*!wall_left + right*!wall_right)* !shoot *!hit*!die*!has_died *!special;

if(abs(hspeed)&gt;max_speed/(1+water))hspeed = sign(hspeed)*max_speed/(1+water);
hspeed -= dec*(!abs(left+right))*sign(hspeed)*!hit*!die*!has_died*!slide;
if(abs(hspeed)&lt;acc)hspeed = 0;

//jump
if((ground or climb) &amp;&amp; keyboard_check_pressed(global.jump) &amp;&amp; !shoot*!hit*!die*!has_died*!special){
    vspeed = -jump/(1+water);
    ground = false;
    can_double = false;
    climb =false;
    //sound_play(fx_jump);
}
if(keyboard_check_released(global.jump) &amp;&amp; !has_jumped &amp;&amp; !shoot*!hit*!die*!has_died*!special){
    can_double = true;
}
if(keyboard_check_pressed(global.jump)&amp;&amp; can_double &amp;&amp; !shoot*!hit*!die*!has_died*!special){
    vspeed = -jump/(1+water);
    can_double = false;
    has_jumped = true;
    climb =false;
    scr_part_double_jump(x,y)
    with(obj_camera){scr_camera_shake()}
    
}

//hit
if(hit){
    vspeed = 0;
    gravity = 0;
    special = false;
    shoot = false;
}

//climb
up = -keyboard_check(global.up);
down = keyboard_check(global.down);
if(climb){
    hspeed = 0;
    y += 0.5*(up+down);
    vspeed = 0;
    gravity = 0;
    shoot = false;
    special = false;
    has_jumped = false;
}


//shoot
scr_char_shoot();

//special
scr_char_special();

//animations
scr_char_animation();

//push
if((wall_left &amp;&amp; abs(left)) or (wall_right &amp;&amp; right)){
    push = true;
    image_xscale = (-(wall_left &amp;&amp; abs(left)) + (wall_right &amp;&amp; abs(right)))
}else{
    push = false;
}

}
else if(finish){
    hspeed = 2*sign(hspeed);
    vspeed = 0;
    gravity = 0;
    ground  = true;
    scr_char_animation();
}
else{
    image_speed = 0;
    hspeed = 0;
    vspeed = 0;
    gravity = 0;
}

if(start)
{
    sprite_index = spr_char_start;
    image_speed = 12/60;
}
</string>
          </argument>
        </arguments>
      </action>

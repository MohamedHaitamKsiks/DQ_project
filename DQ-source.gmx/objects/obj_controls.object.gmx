if(size&lt;1)size+=0.05;
if(size&gt;=1 &amp;&amp;global.control){
    size = 1;
    up = -keyboard_check_pressed(global.up);
    down = keyboard_check_pressed(global.down);
    select += (up+down)*!selected;
    if(select&gt;8)select=0;
    if(select&lt;0)select=8;
    
    /*if((abs(left) or right) &amp;&amp; selected){
        sound_play(fx_select);
    }*/
    if(abs(up) or down){
        sound_play(fx_select);
    }
    
    if(keyboard_check_pressed(global.enter) &amp;&amp; global.can_menu){
    
        if(select == 8){
            global.control = false;
            global.can_menu = false;
            sound_play(fx_confirm);
        }
        else{
            if(!selected){
                selected = true;
                can = false;
            }
            sound_play(fx_select);
        }
        
    }
    
    if(keyboard_check_released(global.enter))can = true;
    
    if(keyboard_check_pressed(vk_anykey) &amp;&amp; selected &amp;&amp; can){
    sound_play(fx_select);
        if(select == 0)global.left = keyboard_key;
        if(select == 1)global.right = keyboard_key;
        if(select == 2)global.up = keyboard_key;
        if(select == 3)global.down = keyboard_key;
        if(select == 4)global.shoot = keyboard_key;
        if(select == 5)global.special = keyboard_key;
        if(select == 6)global.jump = keyboard_key;
        if(select == 7)global.enter = keyboard_key;
        selected = false;
        can = false;
    }
}

</string>

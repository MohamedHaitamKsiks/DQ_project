if(size&lt;1)size+=0.05;
if(size&gt;=1 &amp;&amp; global.profil &amp;&amp; !global.control){
    size = 1;
    up = -keyboard_check_pressed(global.up);
    down = keyboard_check_pressed(global.down);
    
    left = -keyboard_check_pressed(global.left);
    right = keyboard_check_pressed(global.right);
    
    select += (up+down)*!selected;
    if(select&gt;4)select = 0;
    if(select&lt;0)select = 4;
    if((abs(left) or right) &amp;&amp; selected){
        sound_play(fx_select);
    }
    if(abs(up) or down){
        sound_play(fx_select);
    }
    
    if(keyboard_check_pressed(global.enter) &amp;&amp; global.can_menu &amp;&amp; !obj_in_name.insert){
    sound_play(fx_confirm);
        global.can_menu = false;
        if(select == 0){
            scr_data_score_default();
            global.name = "Wizard";
            scr_goto_level(room_next(room));
            global.profil = false;
            global.can_menu = false;
            select = 0;
            selected = false;
            global.prof =  0;
        }
        if(select &gt;=1 &amp;&amp; select&lt;4){
            if(!selected)
                selected = true;
            else{
                if(s_hor==0){
                scr_data_score_default();
                    //load/new
                    global.can_menu = false;
                    ini_open("save_file_"+string(select)+".ini");
                    saved = real(ini_read_string("save","save",0));
                    ini_close();
                    if(saved){
                        scr_data_load_game(string(select));
                        global.prof = select;
                        if(scr_data_level_check(stg_level_0))scr_goto_level(stg_map);
                        else scr_goto_level(stg_level_0);
                    }
                    else{
                        obj_in_name.insert = true;
                        keyboard_string = "";
                    }
                }
                if(s_hor==1){
                    //del
                    scr_data_new_game(string(select));
                    ini_open("save_file_"+string(obj_profil.select)+".ini");
                    ini_write_real("save","save",0);
                    ini_close();
                    
                    global.can_menu = false;
                    
                }
                if(s_hor==2){
                    //back
                    selected = false;
                    global.can_menu = false;
                }

            }

        }
        if(select ==4){
            global.profil = false;
            global.can_menu = false;
            select = 0;
            selected = false;
        }
    }
    if(selected){
        s_hor += left+right;
        if(s_hor&gt;2)s_hor = 2;
        if(s_hor&lt;0)s_hor = 0;
        
    }
}

